> checkIO <- function(name) {
+   cat("checking IO for \'", name, "\' ... ", sep="")
+   x = get(name)
+   y = FALSE
+   if ( !is.null(x) ) {
+     y <- try({
+       xx <- readImage(writeImage(x, tempfile("", fileext = ".tif")))
+       dimnames(xx) <- dimnames(x)
+       identical(x, xx)
+       }, silent=TRUE)
+   }
+   if ( isTRUE(y) ) cat("PASS\n") else cat("FAIL\n")
+   invisible(y)
+ }
