useDynLib(EBImage)
export(
    Image, is.Image, stopIfNotImage, read.image, choose.image,
    TrueColor, Grayscale, morphKern 
)
exportClasses(
    Image
)
exportMethods(
    copy, display, write.image, header, assert, image, combine,
    as.array, as.matrix, print, show, "[", channel, hist,

    features,
    colorMode, "colorMode<-",
    fileName, "fileName<-", 
    compression, "compression<-",
    resolution, "resolution<-",
    sampleFilter, "sampleFilter<-",
    imageData, "imageData<-",

    blur, gblur, contrast, denoise, despeckle, edge, enhance, equalize,
    cgamma, mediansmooth, noise, resize, rotate, resample, segment,
    sharpen, umask, thresh, athresh, cthresh, affinet, modulate, negate, 
    normalize, normalize2, fill, flip, flop, erode, dilate, opening, closing,
    distmap, watershed
    
)
