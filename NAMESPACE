    Image, Drawable, DrawableCircle, DrawableLine, DrawableRect, DrawableEllipse
)
exportMethods(
    assert, copy, write.image, display, channels, normalize,
    as.array, summary, "[", show, print, plot.image,

    toGray, toRGB, toX11char, toRed, toGreen, toBlue, getRed, getGreen, getBlue, 
    add2RGB, sub2RGB, scale2RGB
)
